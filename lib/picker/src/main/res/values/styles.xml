<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:tools="http://schemas.android.com/tools" >

    <!-- Do not change style name as it is being used to override 'monthandyearpicker' library's theme. -->
    <style name="MonthPickerDialogStyle" tools:ignore="UnusedResources">
        <!-- Background -->
        <item name="monthBgColor">@color/colorBackground</item>
        <item name="monthBgSelectedColor">@color/monthBgSelectedColor</item>
        <!-- Font -->
        <item name="monthFontColorNormal">?android:attr/textColorPrimary</item>
        <!-- Header -->
        <item name="headerBgColor">@color/headerBgColor</item>
        <!-- Dialog -->
        <item name="dialogActionButtonColor">@color/dialogActionButtonColor</item>
    </style>

</resources>
